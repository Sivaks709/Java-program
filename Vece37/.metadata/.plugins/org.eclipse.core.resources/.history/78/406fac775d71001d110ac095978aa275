package equals;

public class Book {

	double price=78;
	int id=12;
	String name="Java";//456454
	@Override
	public int hashCode()
	{
		return price+id+name.hashCode();
	}
}
class student {
	
	int sid=12;
	double marks=78;
	String name="Java";
	@Override
	public int hashCode()
	{
		return sid+marks+name.hashCode();
	}
	
}
